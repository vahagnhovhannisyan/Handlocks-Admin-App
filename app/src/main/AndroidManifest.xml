<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.handlocksadminapp">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/admin"
        android:label="@string/app_name"
        android:roundIcon="@drawable/admin"
        android:supportsRtl="true"
        android:theme="@style/Theme.HandLocksAdminApp"
        tools:targetApi="31">
        <activity
            android:name=".activities.AboutUserActivity"
            android:exported="false" />
        <activity
            android:name=".ui.UsersActivity"
            android:exported="false" />
        <activity
            android:name=".activities.AboutOfferedProductActivity"
            android:exported="false" />
        <activity
            android:name=".activities.AddOfferedProductActivity"
            android:exported="false" />
        <activity
            android:name=".ui.OffersActivity"
            android:exported="false" />
        <activity
            android:name=".ui.NewProductsActivity"
            android:exported="false" />
        <activity
            android:name=".activities.AboutProductActivity"
            android:exported="false" />
        <activity
            android:name=".activities.AddProductActivity"
            android:exported="false" />
        <activity
            android:name=".activities.AboutNewProductActivity"
            android:exported="false" />
        <activity
            android:name=".activities.AddNewProductActivity"
            android:exported="false" />
        <activity
            android:name=".ui.ProductsActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>